<!DOCTYPE html>
<html>
  <head>
    <style type="text/css">
      html, body { height: 100%; margin: 0; padding: 0; }
      #map { height: 75%; width: auto;}
    </style>
  </head>
  <body>
    <div id="map"></div>
    

    <script type="text/javascript">

    var map;

    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: {lat:40.73, lng:-73.99},
        zoom: 15
      });
    }

    </script>

    <script async defer
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBlq8_KYCyoiXsUBVeFeVEtKy2j-69QoCs&callback=initMap">
    </script>
  </body>
</html>